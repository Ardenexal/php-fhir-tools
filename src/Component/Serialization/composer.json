{
    "name": "ardenexal/fhir-serialization",
    "version": "0.0.3",
    "description": "PHP library for FHIR JSON serialization, deserialization, and validation",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "symfony/serializer": "^6.4|^7.4",
        "symfony/validator": "^6.4|^7.4",
        "symfony/property-access": "^6.4|^7.4",
        "symfony/property-info": "^6.4|^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "giorgiosironi/eris": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ardenexal\\FHIRTools\\Component\\Serialization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ardenexal\\FHIRTools\\Component\\Serialization\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}