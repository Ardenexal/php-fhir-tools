{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "amphp/http-client": "5",
        "nette/php-generator": "^4.2.0",
        "symfony/console": "^7.4",
        "symfony/dotenv": "^7.4",
        "symfony/filesystem": "^7.4",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.4",
        "symfony/http-client": "^7.4",
        "symfony/intl": "^7.4",
        "symfony/polyfill-php84": "^v1.33.0",
        "symfony/polyfill-php85": "^v1.33.0",
        "symfony/runtime": "^7.4",
        "symfony/validator": "^7.4",
        "symfony/yaml": "^7.4"
    },
    "require-dev": {
        "laravel/pint": "^1.26",
        "phpstan/phpstan": "^2.1.33",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^12.5.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ardenexal\\FHIRTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ardenexal\\FHIR-PHP-Model-Generator\\Tests\\": "tests/",
            "Ardenexal\\FHIRTools\\R4B\\": "output/Ardenexal/FhirTools/R4B/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "php ./vendor/bin/phpstan.phar analyse --configuration=phpstan.neon --memory-limit=-1 -v",
        "lint": "php ./vendor/bin/pint -v --parallel --max-processes=4",
        "test": "php ./vendor/phpunit/phpunit/phpunit",
        "generate-models": "php ./bin/console fhir:generate -vvv"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
